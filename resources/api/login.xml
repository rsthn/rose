
conn.execQuery ("INSERT INTO ##users SET name=?, email=?", ...form.values);

RetrieveData("SELECT * FROM ##users").forEach((item) => (
	Echo(item.name);
));

(auth-required)

(validate
	(field name
		(required true)
		(pattern (Strings.Patterns.Name) errormsg "Incorrect Name")
	)

	(field email
		(required true)
		(pattern (Strings.Patterns.Email) errmsg "Invalid Email Address")
	)
)

(retrieve table "SELECT * FROM ##users")

(foreach i in table
	(echo (i.name))
)
